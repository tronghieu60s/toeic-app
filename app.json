{
  "expo": {
    "name": "TOEIC Essential Words",
    "slug": "toeic-ew",
    "version": "0.0.6",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "toeic-ew",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "ON_ERROR_RECOVERY"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.tronghieuit.toeicew",
      "versionCode": 10,
      "permissions": [
        "VIBRATE"
      ],
      "config": {
        "googleMobileAdsAppId": "ca-app-pub-7154201935109039~5622745585"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}